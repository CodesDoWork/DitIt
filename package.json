{
    "name": "todo-app",
    "version": "1.0.0",
    "license": "MIT",
    "author": "Justin Konratt",
    "scripts": {
        "start": "npm run start:backend && npm run start:frontend",
        "start:backend": "node dist/apps/backend/main.js",
        "start:frontend": "npx serve -s dist/apps/frontend",
        "dev": "nx run-many --all --target=serve --parallel",
        "dev:backend": "nx run backend:serve",
        "dev:frontend": "nx run frontend:serve",
        "build:dev": "nx build backend && nx build frontend",
        "build:prod": "nx build backend --prod && nx build frontend --prod",
        "test": "nx run-many --all --target=test --parallel",
        "lint": "nx workspace-lint && nx run-many --all --target=lint --parallel",
        "docker:build": "docker build . -t todo_app",
        "docker:run": "docker run -p 80:80 -p 3333:3333 --name todo_app -v todo_app:/data/db -d todo_app"
    },
    "private": true,
    "devDependencies": {
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@nrwl/cli": "14.3.6",
        "@nrwl/cypress": "14.3.6",
        "@nrwl/eslint-plugin-nx": "14.3.6",
        "@nrwl/jest": "14.3.6",
        "@nrwl/linter": "14.3.6",
        "@nrwl/nest": "^14.3.6",
        "@nrwl/node": "14.3.6",
        "@nrwl/react": "^14.3.6",
        "@nrwl/web": "14.3.6",
        "@nrwl/workspace": "14.3.6",
        "@testing-library/react": "^12.0.0",
        "@types/bcrypt": "^5.0.0",
        "@types/jest": "27.4.1",
        "@types/node": "16.11.7",
        "@types/passport-jwt": "^3.0.6",
        "@types/pluralize": "^0.0.29",
        "@types/react": "18.0.13",
        "@types/react-beautiful-dnd": "^13.1.2",
        "@types/react-dom": "18.0.5",
        "@types/react-router-dom": "^5.3.3",
        "@types/universal-cookie": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "~5.24.0",
        "@typescript-eslint/parser": "~5.24.0",
        "babel-jest": "27.5.1",
        "cypress": "^9.1.0",
        "eslint": "~8.15.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-react": "7.30.0",
        "eslint-plugin-react-hooks": "4.6.0",
        "jest": "27.5.1",
        "nx": "14.3.6",
        "prettier": "^2.6.2",
        "react-test-renderer": "^17.0.0",
        "ts-jest": "27.1.4",
        "ts-node": "~10.8.0",
        "typescript": "~4.7.2"
    },
    "dependencies": {
        "@nestjs/common": "^8.0.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/jwt": "^8.0.1",
        "@nestjs/passport": "^8.2.2",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/swagger": "^5.2.1",
        "@types/react-router": "^5.1.18",
        "axios": "^0.27.2",
